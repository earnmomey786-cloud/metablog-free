{
   "name": "metablog-free",
   "version": "0.0.1-alpha",
   "private": true,
   "workspaces": [
      "apps/*",
      "packages/*"
   ],
   "scripts": {
      "build": "cd apps/site && npm run build",
      "dev": "cd apps/site && npm run dev",
      "start": "cd apps/site && npm run start",
      "lint": "turbo run lint",
      "format": "prettier --write . --ignore-path .prettierignore",
      "turbo:build": "turbo run build",
      "turbo:dev": "turbo run dev"
   },
   "devDependencies": {
      "eslint-config-custom": "workspace:*",
      "prettier": "latest",
      "turbo": "latest"
   },
   "engines": {
      "node": ">=14.0.0"
   },
   "dependencies": {},
   "packageManager": "pnpm@7.29.1"
}
